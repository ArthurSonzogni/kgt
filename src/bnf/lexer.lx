/[ \t\v\f\r]+/;

'::=' -> $equals;
'|'   -> $alt;

# Newlines are whitespace, except for a double newline,
# which acts as a production separator.
"\n\n" -> $sep;
"\n";

"<" .. ">" -> $name {
	/[-a-z0-9_]/i -> $char;
}

"'" .. "'" -> $literal {
	/[^']/ -> $char;
}

'"' .. '"' -> $literal {
	/[^"]/ -> $char;
}

'""' -> $empty;
"''" -> $empty;

